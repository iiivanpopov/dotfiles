{
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 20,
  "editor.fontWeight": "600",
  "editor.fontLigatures": false,
  "editor.lineHeight": 26,
  "editor.inlayHints.fontFamily": "JetBrains Mono",
  "editor.codeLensFontFamily": "JetBrains Mono",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontWeight": 600,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "explicit",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.glyphMargin": false,
  "editor.folding": true,
  "editor.lightbulb.enabled": "off",
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.guides.bracketPairs": false,
  "editor.guides.indentation": false,
  "editor.inlayHints.enabled": "off",
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.scrollBeyondLastLine": true,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "same",
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.linkedEditing": true,
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "editor.accessibilitySupport": "off",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.fixAll.stylelint": "always"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "workbench.iconTheme": "flow-dawn",
  "workbench.activityBar.location": "hidden",
  "workbench.statusBar.visible": true,
  "workbench.layoutControl.enabled": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.tree.enableStickyScroll": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.tabSizing": "shrink",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 2,
  "window.commandCenter": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.density.editorTabHeight": "compact",
  "window.openFilesInNewWindow": "off",
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": true,
  "zenMode.hideLineNumbers": true,
  "zenMode.showTabs": "none",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.stickyScroll.enabled": false,
  "terminal.integrated.environmentChangesRelaunch": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "explorer.excludeGitIgnore": false,
  "files.eol": "\n",
  "files.defaultLanguage": "plaintext",
  "files.exclude": {
    "**/.cursor": true,
    "**/.idea": true,
    "**/.nuxt": true,
    "**/.tanstack": true,
    "**/.vscode": true,
    "**/bun.lock": true,
    "**/Cargo.lock": true,
    "**/certs": true,
    "**/credentials": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/prisma/generated": true,
    "**/routeTree.gen.ts": true,
    "**/target": true,
    "**/tsconfig.node.tsbuildinfo": true,
    "**/.husky/_": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  },
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "files.associations": {
    "prepare-commit-msg": "shellscript"
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true
  },
  "breadcrumbs.enabled": false,
  "eslint.lintTask.command": "bun eslint",
  "eslint.rules.customizations": [
    { "rule": "style/*", "severity": "off", "fixable": true },
    { "rule": "format/*", "severity": "off", "fixable": true },
    { "rule": "*-indent", "severity": "off", "fixable": true },
    { "rule": "*-spacing", "severity": "off", "fixable": true },
    { "rule": "*-spaces", "severity": "off", "fixable": true },
    { "rule": "*-order", "severity": "off", "fixable": true },
    { "rule": "*-dangle", "severity": "off", "fixable": true },
    { "rule": "*-newline", "severity": "off", "fixable": true },
    { "rule": "*quotes", "severity": "off", "fixable": true },
    { "rule": "*semi", "severity": "off", "fixable": true }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml",
    "toml",
    "xml",
    "gql",
    "graphql",
    "astro",
    "svelte",
    "css",
    "less",
    "scss",
    "pcss",
    "postcss"
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsserver.maxTsServerMemory": 8192,
  "javascript.autoClosingTags": true,
  "html.autoClosingTags": true,
  "npm.packageManager": "bun",
  "npm.scriptRunner": "bun",
  "npm.scriptHover": false,
  "css.lint.unknownAtRules": "ignore",
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "cSpell.enabled": true,
  "cSpell.autoFormatConfigFile": true,
  "cSpell.language": "en, uk, ru",
  "chat.mcp.gallery.enabled": true,
  "chat.viewSessions.orientation": "stacked",
  "chat.editor.fontFamily": "JetBrainsMono",
  "debug.javascript.codelens.npmScripts": "never",
  "docker.extension.dockerEngineAvailabilityPrompt": false,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "extensions.ignoreRecommendations": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "workbench.colorTheme": "Catppuccin Latte"
}
