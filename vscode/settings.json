{
  "window.commandCenter": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.density.editorTabHeight": "compact",
  "window.openFilesInNewWindow": "off",
  "window.zoomLevel": 1.5,
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.iconTheme": "flow-dim",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.enableStickyScroll": false,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "explicit",
  "editor.cursorStyle": "line-thin",
  "editor.detectIndentation": true,
  "editor.folding": false,
  "editor.glyphMargin": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": false,
  "editor.scrollBeyondLastLine": true,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "same",
  "editor.fontFamily": "0xProto Nerd Font Mono",
  "editor.fontSize": 20,
  "editor.fontLigatures": false,
  "editor.fontWeight": "400",
  "editor.lineHeight": 26,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.bracketPairColorization.enabled": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.fixAll.stylelint": "always"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class",
          "keyword",
          "constant",
          "storage.modifier",
          "storage.type.class.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "breadcrumbs.enabled": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.defaultLanguage": "plaintext",
  "files.eol": "\n",
  "explorer.excludeGitIgnore": false,
  "files.exclude": {
    "**/.cursor": true,
    "**/pnpm-lock.yaml": true,
    "**/.idea": true,
    "**/.nuxt": true,
    "**/.vscode": true,
    "**/Cargo.lock": true,
    "**/bun.lock": true,
    "**/certs": true,
    "**/credentials": true,
    "**/dist": true,
    "**/drizzle": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/prisma/generated": true,
    "**/tsconfig.node.tsbuildinfo": true
  },
  "zenMode.centerLayout": false,
  "zenMode.showTabs": "none",
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "html.autoClosingTags": true,
  "html.completion.attributeDefaultValue": "doublequotes",
  "javascript.autoClosingTags": true,
  "javascript.format.semicolons": "remove",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.autoClosingTags": true,
  "typescript.format.semicolons": "remove",
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml",
    "toml",
    "xml",
    "gql",
    "graphql",
    "astro",
    "svelte",
    "css",
    "less",
    "scss",
    "pcss",
    "postcss"
  ],
  "eslint.rules.customizations": [
    { "rule": "style/*", "severity": "off", "fixable": true },
    { "rule": "format/*", "severity": "off", "fixable": true },
    { "rule": "*-indent", "severity": "off", "fixable": true },
    { "rule": "*-spacing", "severity": "off", "fixable": true },
    { "rule": "*-spaces", "severity": "off", "fixable": true },
    { "rule": "*-order", "severity": "off", "fixable": true },
    { "rule": "*-dangle", "severity": "off", "fixable": true },
    { "rule": "*-newline", "severity": "off", "fixable": true },
    { "rule": "*quotes", "severity": "off", "fixable": true },
    { "rule": "*semi", "severity": "off", "fixable": true }
  ],
  "cSpell.autoFormatConfigFile": true,
  "cSpell.enabled": true,
  "cSpell.language": "en",

  "npm.packageManager": "pnpm",
  "stylelint.packageManager": "pnpm",
  "editor.inlayHints.enabled": "off",
  "editor.lineNumbers": "off",
  "editor.stickyScroll.enabled": false,
  "editor.wordWrap": "bounded",
  "editor.find.addExtraSpaceOnTop": false,
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
  "workbench.preferredHighContrastColorTheme": "Catppuccin Mocha",
  "workbench.preferredHighContrastLightColorTheme": "Catppuccin Latte",
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "workbench.statusBar.visible": false,
  "workbench.view.showQuietly": {
    "workbench.panel.output": false
  },
  "workbench.editor.autoLockGroups": {
    "mainThreadWebview-simpleBrowser.view": false,
    "mainThreadWebview-browserPreview": false,
    "terminalEditor": false,
    "workbench.editor.processExplorer": false,
    "workbench.editor.chatSession": false
  },
  "workbench.editor.defaultBinaryEditor": "default",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.tabActionCloseVisibility": false,
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.environmentChangesRelaunch": false,
  "terminal.integrated.fontFamily": "Cascadia Mono",
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.gpuAcceleration": "off",
  "catppuccin.accentColor": "lavender",
  "npm.scriptHover": false,
  "js/ts.implicitProjectConfig.target": "ESNext",
  "debug.javascript.codelens.npmScripts": "never"
}