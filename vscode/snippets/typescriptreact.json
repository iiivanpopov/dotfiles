{
  "Import CSS Module": {
    "prefix": ["imcss"],
    "body": ["import styles from './${TM_FILENAME_BASE}.module.css'"]
  },

  "React Component": {
    "prefix": ["rfc"],
    "body": [
      "export function ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}() {",
      "  return <div>$0</div>",
      "}"
    ]
  },

  "React Component with Props": {
    "prefix": ["rfcp"],
    "body": [
      "interface ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props {",
      "  $1",
      "}",
      "",
      "export function ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}({ $2 }: ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props) {",
      "  return <div>$3</div>",
      "}"
    ]
  },

  "React Component with Props (children Default)": {
    "prefix": ["rfcpc"],
    "body": [
      "import { ReactNode } from 'react'",
      "",
      "interface ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props {",
      "  children: ReactNode",
      "}",
      "",
      "export function ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}({ children }: ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props) {",
      "  $0",
      "  ",
      "  return <div>{children}</div>",
      "}"
    ]
  },


  "React Arrow Component": {
    "prefix": ["rafc"],
    "body": [
      "export const ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/} = () => {",
      "  return <div>$0</div>",
      "}"
    ]
  },

  "React Arrow Component with Props": {
    "prefix": ["rafcp"],
    "body": [
      "interface ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props {",
      "  $1",
      "}",
      "",
      "export const ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/} = ({ $2 }: ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props) => {",
      "  return <div>$3</div>",
      "}"
    ]
  },

  "React Arrow Component with Props (children Default)": {
    "prefix": ["rafcpc"],
    "body": [
      "import { ReactNode } from 'react'",
      "",
      "interface ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props {",
      "  children: ReactNode",
      "}",
      "",
      "export const ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/} = ({ children }: ${TM_FILENAME_BASE/^(.*)$/${1:/pascalcase}/}Props) => {",
      "  $0",
      "  ",
      "  return <div>{children}</div>",
      "}"
    ]
  },

  "Custom Hook": {
    "prefix": ["hook"],
    "body": [
      "export function ${TM_FILENAME_BASE/^(use-)?(.+)$/use${2:/pascalcase}/}() {",
      "  $0",
      "",
      "  return {",
      "    ",
      "  }",
      "}"
    ]
  }
}